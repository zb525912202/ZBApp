<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  
  <configSections>
    <section name="dataConfiguration" type="Microsoft.Practices.EnterpriseLibrary.Data.Configuration.DatabaseSettings, Microsoft.Practices.EnterpriseLibrary.Data" />
    <section name="cachingConfiguration" type="Microsoft.Practices.EnterpriseLibrary.Caching.Configuration.CacheManagerSettings, Microsoft.Practices.EnterpriseLibrary.Caching" requirePermission="true" />
  </configSections>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  
  <connectionStrings configSource="ConnectionStrings.config" />

  <appSettings configSource="appSettings.config"/>
  <dataConfiguration defaultDatabase="CommonModelContainer" />

  <cachingConfiguration defaultCacheManager="">
    <cacheManagers>
      <add name="WebPaperPackageQuestionCacheManager" type="Microsoft.Practices.EnterpriseLibrary.Caching.CacheManager, Microsoft.Practices.EnterpriseLibrary.Caching" expirationPollFrequencyInSeconds="300" maximumElementsInCacheBeforeScavenging="10000" numberToRemoveWhenScavenging="100" backingStoreName="NullBackingStore" />
      <add name="PaperReplyStateCacheManager" type="Microsoft.Practices.EnterpriseLibrary.Caching.CacheManager, Microsoft.Practices.EnterpriseLibrary.Caching" expirationPollFrequencyInSeconds="60" maximumElementsInCacheBeforeScavenging="10000" numberToRemoveWhenScavenging="100" backingStoreName="NullBackingStore" />
    </cacheManagers>
    <backingStores>
      <add type="Microsoft.Practices.EnterpriseLibrary.Caching.BackingStoreImplementations.NullBackingStore, Microsoft.Practices.EnterpriseLibrary.Caching" name="NullBackingStore" />
    </backingStores>
  </cachingConfiguration>
  
  <system.data>
    <DbProviderFactories>
      <add name="ZB.Framework.DataAccess.MySql" invariant="ZB.Framework.DataAccess.MySql" description="ZB.Framework.DataAccess.MySql Provider" type="ZB.Framework.DataAccess.MySql.MySqlDatabase, ZB.Framework.DataAccess, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null" />
      <add name="ZB.Framework.DataAccess.Oracle" invariant="ZB.Framework.DataAccess.Oracle" description="ZB.Framework.DataAccess.Oracle Provider" type="ZB.Framework.DataAccess.Oracle.OracleDatabase, ZB.Framework.DataAccess, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null" />
    </DbProviderFactories>
  </system.data>
  
</configuration>